{
  "name": "angular-leaflet-dev",
  "version": "0.0.14",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build --prod",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build --prod lib",
    "publish-lib": "cd dist/lib && npm publish",
    "deploy-lib": "npm run build-lib && npm run publish-lib",
